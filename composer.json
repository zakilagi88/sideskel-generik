{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "awcodes/filament-table-repeater": "^3.0",
        "bezhansalleh/filament-shield": "^3.2",
        "coolsam/flatpickr": "^3.1",
        "filament/filament": "^3.2",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "filament/spatie-laravel-tags-plugin": "^3.2",
        "flowframe/laravel-trend": "^0.2",
        "guava/filament-clusters": "^1.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.8",
        "leandrocfe/filament-apex-charts": "^3.1",
        "livewire/livewire": "^3.4",
        "maatwebsite/excel": "^3.1",
        "owenvoke/blade-fontawesome": "^2.4",
        "psr/simple-cache": "^2.0",
        "riodwanto/filament-ace-editor": "^1.1",
        "saade/filament-adjacency-list": "^3.2",
        "saade/filament-fullcalendar": "^3.2",
        "spatie/laravel-markdown": "^2.4",
        "spatie/laravel-settings": "^3.3",
        "staudenmeir/belongs-to-through": "^2.5",
        "staudenmeir/laravel-adjacency-list": "^1.2",
        "tapp/filament-auditing": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.10",
        "doctrine/dbal": "^3.8",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.2",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
